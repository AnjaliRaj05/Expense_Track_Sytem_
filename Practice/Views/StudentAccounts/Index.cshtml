@model IEnumerable<Practice.Models.StudentAccount>

@{
    ViewData["Title"] = "Student Data";
}

<h2>Student Data</h2>

<table class ="tablle table-striped">
  <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Class</th>
            <th>Fee</th>
        </tr>
  </thead>
    
    @foreach (var item in ViewBag.studentList)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Class</td>
            <td>@item.Fee</td>
        </tr>
    }
</table>
